{
  "name": "hub",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start  --no-open",
    "dev": "nodemon",
    "build": "docusaurus clear && npm run apidocs && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clean-api-docs all && docusaurus clear",
    "serve": "npm run build && docusaurus serve",
    "apidocs": "docusaurus gen-api-docs all && npm run oas:bundle",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "oas:bundle": "node oas/bundle-oas.js"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "12.0.1",
    "@docusaurus/core": "3.7.0",
    "@docusaurus/plugin-content-docs": "^3.7.0",
    "@docusaurus/plugin-ideal-image": "^3.7.0",
    "@docusaurus/plugin-sitemap": "^3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@docusaurus/theme-mermaid": "^3.7.0",
    "@mdx-js/react": "^3.0.0",
    "@types/react-helmet": "^6.1.11",
    "@types/styled-components": "^5.1.34",
    "clsx": "^2.0.0",
    "docusaurus-plugin-openapi-docs": "file:./.dependencies/docusaurus-plugin-openapi-docs-v4.3.7.tgz",
    "docusaurus-theme-openapi-docs": "file:./.dependencies/docusaurus-theme-openapi-docs-v4.3.7.tgz",
    "panzoom": "^9.4.3",
    "prism-react-renderer": "^2.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-helmet": "^6.1.0",
    "styled-components": "6.1.17",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "nodemon": "^3.1.9",
    "typescript": "5.8.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
